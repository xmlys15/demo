{
  "log": {
    "disabled": false,
    "level": "info",
    "timestamp": true
  },
  "dns": {
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/15",
      "inet6_range": "fc00::/18"
    },
    "servers": [
      {
        "tag": "google",
        "address": "tls://8.8.8.8",
        "detour": "🚀 默认出站"
      },
      {
        "tag": "local",
        "address": "223.5.5.5",
        "detour": "direct"
      },
      {
        "tag": "remote",
        "address": "fakeip"
      }
    ],
    "rules": [
      {
        "outbound": "any", 
        "server": "local"
      },
      {
        "rule_set": "geosite-cn",
        "server": "local"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "remote"
      }
    ],
    "independent_cache": true
  },
  "inbounds": [
    {
      "tag": "tun-in",
      "type": "tun",
      "address": [
        "172.19.0.0/30",
        "fdfe:dcba:9876::0/126"
      ],
      "stack": "system",
      "auto_route": true,
      "strict_route": true,
      "platform": {
        "http_proxy": {
          "enabled": true,
          "server": "127.0.0.1",
          "server_port": 7890
        }
      }
    },
    {
      "tag": "mixed-in",
      "type": "mixed",
      "listen": "127.0.0.1",
      "listen_port": 7890
    }
  ],
  "outbounds": [
    {
      "tag": "🚀 默认出站",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "exclude",
          "keywords": [
            "剩|过|直"
          ]
        }
      ],
      "interrupt_exist_connections": true
    },
    {
      "tag": "OpenAI",
      "type": "selector",
      "outbounds": [
        "🚀 默认出站",
        "📌 单选节点",
        "🇺🇲 美国节点",
        "🇯🇵 日本节点",
        "🇭🇰 香港节点",
        "🇸🇬 新加坡节点",
        "🇼🇸 台湾节点",
        "direct"
      ],
      "interrupt_exist_connections": true
    },
    {
      "tag": "Instagram",
      "type": "selector",
      "outbounds": [
        "🚀 默认出站",
        "📌 单选节点",
        "🇺🇲 美国节点",
        "🇯🇵 日本节点",
        "🇭🇰 香港节点",
        "🇸🇬 新加坡节点",
        "🇼🇸 台湾节点",
        "direct"
      ],
      "interrupt_exist_connections": true
    },
    {
      "tag": "📌 单选节点",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "exclude",
          "keywords": [
            "日|美|新|台|港|剩|过|直"
          ]
        }
      ],
      "interrupt_exist_connections": true
    },
    {
      "tag": "🇺🇲 美国节点",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "US|us|美国|美|United States"
          ]
        }
      ],
      "interrupt_exist_connections": true
    },
    {
      "tag": "🇯🇵 日本节点",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "JP|jp|日本|日|Japan"
          ]
        }
      ],
      "interrupt_exist_connections": true
    },
    {
      "tag": "🇭🇰 香港节点",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "HK|hk|香港|港|HongKong"
          ]
        }
      ],
      "interrupt_exist_connections": true
    },
    {
      "tag": "🇸🇬 新加坡节点",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "新|sg|sgp"
          ]
        }
      ],
      "interrupt_exist_connections": true
    },
    {
      "tag": "🇼🇸 台湾节点",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "台|tw"
          ]
        }
      ],
      "interrupt_exist_connections": true
    },
    {
      "tag": "direct",
      "type": "direct"
    }
  ],
